<h1>New Client</h1>

<%= form_for :client do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :company %><br>
    <%= f.text_field :company %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

      <p>
    <%= f.label :phone %><br>
    <%= f.text_field :phone %>
  </p>

      <p>
    <%= f.label :project_name %><br>
    <%= f.text_field :project_name %>
  </p>

        <p>
    <%= f.label :project_info %><br>
    <%= f.text_area :project_info %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>